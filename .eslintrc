{
  "rules": {
        "no-var": 0,
        "prettier/prettier": ["error"]
    },
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier"],
   "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 13,
        "requireConfigFile": false,
    },
    "parser": "@babel/eslint-parser",
    "env": {
        "browser": true,
    },


}